<!-- front gear steering -->
  <animation>
    <type>rotate</type>
    <object-name>block_train_avant</object-name>
    <property>controls/flight/rudder</property>
    <interpolation>
      <entry><ind>-1</ind><dep>45</dep></entry>
      <entry><ind>-.9</ind><dep>35</dep></entry>
      <entry><ind>-.8</ind><dep>20</dep></entry>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>.8</ind><dep>-20</dep></entry>
      <entry><ind>.9</ind><dep>-35</dep></entry>
      <entry><ind>1</ind><dep>-45</dep></entry>
    </interpolation>
    <center>
      <x-m>-3.0268</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>


<!-- retracting gears -->
  <animation>
    <type>rotate</type>
    <object-name>block_train_avant</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>110</dep></entry>
      <entry><ind>1.0</ind><dep>0</dep></entry>
    </interpolation>
    <center>
      <x-m>-3.0268</x-m>
      <y-m>0</y-m>
      <z-m>-0.64</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>block_train_gauche</object-name>
    <property>gear/gear[1]/position-norm</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>80</dep></entry>
      <entry><ind>1.0</ind><dep>0</dep></entry>
    </interpolation>
    <center>
      <x-m>-1.43</x-m>
      <y-m>-.9</y-m>
      <z-m>-.5</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>block_train_droit</object-name>
    <property>gear/gear[2]/position-norm</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>80</dep></entry>
      <entry><ind>1.0</ind><dep>0</dep></entry>
    </interpolation>
    <center>
      <x-m>-1.43</x-m>
      <y-m>.9</y-m>
      <z-m>-.5</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>select</type>
    <object-name>block_train_avant</object-name>
    <object-name>block_train_gauche</object-name>
    <object-name>block_train_droit</object-name>
    <condition>
      <greater-than>
        <property>gear/gear/position-norm</property>
        <value>.05</value>
      </greater-than>
    </condition>
  </animation>


<!-- roulement -->
  <animation>
    <type>spin</type>
    <object-name>roue_avant</object-name>
    <property>/gear/gear[0]/rollspeed-ms</property>
    <factor>30</factor>
    <center>
      <x-m>-2.92</x-m>
      <y-m>0</y-m>
      <z-m>-1.104</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>spin</type>
    <object-name>roues_g</object-name>
    <property>/gear/gear[1]/rollspeed-ms</property>
    <factor>30</factor>
    <center>
      <x-m>-1.292</x-m>
      <y-m>0</y-m>
      <z-m>-1.079</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>spin</type>
    <object-name>roues_d</object-name>
    <property>/gear/gear[2]/rollspeed-ms</property>
