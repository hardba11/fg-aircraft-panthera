<!-- suspension train avant -->
  <animation>
    <name>block_jambe_train_avant</name>
    <object-name>train_av.jambe_roue</object-name>
    <object-name>roue_avant</object-name>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>block_jambe_train_avant</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry><ind>0</ind><dep>-10</dep></entry>
      <entry><ind>.09</ind><dep>0</dep></entry>
      <entry><ind>.4</ind><dep>10</dep></entry>
    </interpolation>
    <center>
      <x-m>-3.159</x-m>
      <y-m>0</y-m>
      <z-m>-0.903</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>


<!-- suspension train droit -->
  <animation>
    <name>block_jambe_train_droit</name>
    <object-name>roues_d</object-name>
    <object-name>train.jambe_roue_d</object-name>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>block_jambe_train_droit</object-name>
    <property>gear/gear[2]/compression-norm</property>
    <interpolation>
      <entry><ind>0</ind><dep>-10</dep></entry>
      <entry><ind>.17</ind><dep>0</dep></entry>
      <entry><ind>.4</ind><dep>10</dep></entry>
    </interpolation>
    <center>
      <x-m>-1.479</x-m>
      <y-m>0</y-m>
      <z-m>-0.918</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <name>block_piston_train_droit</name>
    <object-name>train.piston_d</object-name>
    <object-name>train.piston_roue_d</object-name>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>block_piston_train_droit</object-name>
    <property>gear/gear[2]/compression-norm</property>
    <interpolation>
      <entry><ind>0</ind><dep>-2</dep></entry>
      <entry><ind>.17</ind><dep>0</dep></entry>
      <entry><ind>.4</ind><dep>2</dep></entry>
    </interpolation>
    <center>
      <x-m>-1.397</x-m>
      <y-m>0</y-m>
      <z-m>-0.689</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>train.piston_roue_d</object-name>
    <property>gear/gear[2]/compression-norm</property>
    <interpolation>
      <entry><ind>0</ind><dep>0.015</dep></entry>
      <entry><ind>.19</ind><dep>0</dep></entry>
      <entry><ind>.4</ind><dep>-0.03</dep></entry>
    </interpolation>
    <axis>
      <x>0.022</x>
      <y>0</y>
      <z>-0.115</z>
    </axis>
  </animation>


