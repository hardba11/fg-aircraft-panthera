  <!-- ~~~~~~~~~~~~~~~~~~ control left mfd button 1 -->
  <animation>
    <type>pick</type>
    <object-name>btn_mfd_left.b1</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-cycle</command>
        <wrap type="bool">1</wrap>
        <property>/instrumentation/my_aircraft/mfd_left/current_lb1_set</property>
        <value>AP</value>
        <value>set-hdg</value>
        <value>set-alt</value>
        <value>set-speed</value>
      </binding>
    </action>
  </animation>

  <!-- ~~~~~~~~~~~~~~~~~~ control left mfd knob 1 -->
  <animation>
    <type>pick</type>
    <object-name>btn_mfd_left.k1</object-name>
    <action>
      <button>3</button>
      <repeatable>true</repeatable>

      <binding>
        <condition>
          <equals>
            <property>/instrumentation/my_aircraft/mfd_left/current_lb1_set</property>
            <value>set-hdg</value>
          </equals>
        </condition>
        <command>property-adjust</command>
        <property>/autopilot/settings/heading-bug-deg</property>
        <step>1</step>
        <min>0</min>
        <max>360</max>
        <wrap>1</wrap>
      </binding>

      <binding>
        <condition>
          <equals>
            <property>/instrumentation/my_aircraft/mfd_left/current_lb1_set</property>
            <value>set-alt</value>
          </equals>
        </condition>
        <command>property-adjust</command>
        <property>/autopilot/settings/target-altitude-ft</property>
        <step>100</step>
        <min>0</min>
        <max>10000</max>
        <wrap>0</wrap>
      </binding>

      <binding>
        <condition>
          <equals>
            <property>/instrumentation/my_aircraft/mfd_left/current_lb1_set</property>
            <value>set-speed</value>
          </equals>
        </condition>
        <command>property-adjust</command>
        <property>/autopilot/settings/target-speed-kt</property>
        <step>1</step>
        <min>0</min>
        <max>200</max>
        <wrap>0</wrap>
      </binding>

    </action>

    <action>
      <button>4</button>
      <repeatable>true</repeatable>

      <binding>
        <condition>
          <equals>
            <property>/instrumentation/my_aircraft/mfd_left/current_lb1_set</property>
            <value>set-hdg</value>
          </equals>
        </condition>
        <command>property-adjust</command>
        <property>/autopilot/settings/heading-bug-deg</property>
        <step>-1</step>
        <min>0</min>
        <max>360</max>
        <wrap>1</wrap>
      </binding>

      <binding>
        <condition>
          <equals>
            <property>/instrumentation/my_aircraft/mfd_left/current_lb1_set</property>
            <value>set-alt</value>
          </equals>
        </condition>
        <command>property-adjust</command>
        <property>/autopilot/settings/target-altitude-ft</property>
        <step>-100</step>
        <min>0</min>
        <max>10000</max>
        <wrap>0</wrap>
      </binding>

      <binding>
        <condition>
          <equals>
            <property>/instrumentation/my_aircraft/mfd_left/current_lb1_set</property>
            <value>set-speed</value>
          </equals>
        </condition>
        <command>property-adjust</command>
        <property>/autopilot/settings/target-speed-kt</property>
        <step>-1</step>
        <min>0</min>
        <max>200</max>
        <wrap>0</wrap>
      </binding>

    </action>
  </animation>


