  <animation>
    <type>blend</type>
    <object-name>propeller_fast</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor>0.0002</factor>
    <max>0.7</max>
    <min>0</min>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>propeller_fast</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor>-0.008</factor>
    <center>
      <x-m>-4.2528</x-m>
      <y-m>0</y-m>
      <z-m>-0.179864</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>select</type>
    <object-name>train_avant.roue</object-name>
    <object-name>train_principal.roue</object-name>
    <condition>
      <equals>
        <property>gear/gear/position-norm</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

