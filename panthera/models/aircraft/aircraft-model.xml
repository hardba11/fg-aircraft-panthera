<?xml version="1.0" encoding="utf-8"?>

<PropertyList>

  <path>aircraft.ac</path>

  <animation>
    <type>material</type>
    <object-name>aircraft</object-name>
    <object-name>helice.cone</object-name>
    <object-name>porte_ar</object-name>
    <object-name>porte_av</object-name>
    <object-name>propeller_fast</object-name>
    <object-name>train_avant.roue</object-name>
    <object-name>train_principal.roue</object-name>
    <object-name>vitre_ar</object-name>
    <object-name>vitre_av</object-name>
  </animation>

  <effect>
    <inherits-from>Aircraft/panthera/models/effects/glass</inherits-from>
    <object-name>vitre_av</object-name>
    <object-name>vitre_ar</object-name>
  </effect>

  <animation>
    <type>select</type>
    <object-name>vitre_av</object-name>
    <object-name>vitre_ar</object-name>
    <condition>
      <equals>
        <property>sim/current-view/internal</property>
        <value>0</value>
      </equals>
    </condition>
  </animation>

  <!--animation>
